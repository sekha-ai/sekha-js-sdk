{
  "name": "@sekha-ai/sdk",
  "version": "0.2.0",
  "type": "module",
  "description": "Official JavaScript/TypeScript SDK for Sekha AI Memory System - Unified access to REST, MCP, and LLM Bridge APIs with full type safety",
  "author": "Sekha AI <jeff@sekha-ai.dev>",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 50",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "ai",
    "memory",
    "sekha",
    "llm",
    "rag",
    "typescript",
    "sdk",
    "conversation",
    "semantic-search",
    "vector-search",
    "embeddings",
    "mcp",
    "model-context-protocol",
    "chatgpt",
    "openai",
    "anthropic",
    "memory-management",
    "context-assembly",
    "multi-modal"
  ],
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sekha-ai/sekha-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/sekha-ai/sekha-js-sdk/issues"
  },
  "homepage": "https://sekha.dev",
  "documentation": "https://docs.sekha.dev",
  "changelog": "https://github.com/sekha-ai/sekha-js-sdk/blob/main/CHANGELOG.md",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/sekha-ai"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.16",
    "eslint": "^10.0.0",
    "happy-dom": "^20.3.9",
    "typescript": "^5.9.0",
    "vitest": "^4.0.18"
  }
}