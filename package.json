{
  "name": "@sekha/sdk",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript SDK for Sekha AI Memory Controller",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.cjs.json",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "ai",
    "memory",
    "sekha",
    "typescript"
  ],
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sekha-ai/sekha-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/sekha-ai/sekha-js-sdk/issues"
  },
  "homepage": "https://github.com/sekha-ai/sekha-js-sdk#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  }
}
